@reboot sleep 30 && echo "$(/usr/bin/date --rfc-3339=seconds) bisyncing notes" >> /home/mammadu/.rclone/logs/logfile && /usr/bin/rclone bisync gdrive:notes /home/mammadu/Documents/notes/ --drive-skip-gdocs --resilient --recover --max-lock 2m --conflict-resolve newer >> /home/mammadu/.rclone/logs/logfile 2>&1

